{
  "name": "@podai/cli",
  "version": "1.0.0",
  "description": "Interactive CLI for PodAI Protocol",
  "type": "module",
  "bin": {
    "podai": "./bin/podai.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun test",
    "test:e2e": "bun test --timeout 30000 tests/e2e/",
    "test:unit": "bun test tests/unit/",
    "test:integration": "bun test tests/integration/",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "7.5.3",
    "@inquirer/select": "4.2.3",
    "@inquirer/input": "4.1.12",
    "@inquirer/confirm": "5.1.12",
    "@inquirer/checkbox": "4.1.8",
    "@inquirer/password": "4.0.15",
    "commander": "14.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "boxen": "^8.0.1",
    "figlet": "^1.8.0",
    "gradient-string": "^3.0.0",
    "nanospinner": "^1.1.0",
    "picocolors": "^1.1.1",
    "terminal-link": "4.0.0",
    "@solana/addresses": "2.1.1",
    "@solana/rpc": "2.1.1",
    "@solana/rpc-types": "2.1.1",
    "@solana/signers": "2.1.1",
    "@solana/codecs": "2.1.1",
    "bs58": "^6.0.0",
    "@podai/sdk": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.0.7",
    "@types/figlet": "^1.5.8",
    "@types/gradient-string": "^1.1.6",
    "typescript": "^5.0.0",
    "eslint": "^9.30.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "tsx": "^4.19.2",
    "bun-types": "^1.1.34"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "cli",
    "podai",
    "agent",
    "blockchain",
    "interactive",
    "ai",
    "agents",
    "solana",
    "pod-protocol"
  ],
  "author": "PodAI Team",
  "license": "MIT"
} 