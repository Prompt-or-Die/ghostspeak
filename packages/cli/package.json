{
  "name": "@ghostspeak/cli",
  "version": "1.0.7",
  "description": "Modern CLI for ghostspeak - Autonomous Agent Commerce Protocol",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ghostspeak": "./bin/ghostspeak.js",
    "gs": "./bin/ghostspeak.js"
  },
  "scripts": {
    "build": "bun run clean && bun run compile",
    "compile": "bun build src/index.ts --outdir dist --target node --minify",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "lint": "echo 'Linting skipped - lightweight CLI'",
    "clean": "rimraf dist coverage",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "prepack": "bun run build"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "keywords": [
    "cli",
    "solana",
    "blockchain",
    "ai-agents",
    "ghostspeak"
  ],
  "author": "ghostspeak Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.15"
  },
  "devDependencies": {
    "@types/node": "24.0.12",
    "rimraf": "6.0.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "conf": "14.0.0"
  }
}
