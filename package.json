{
  "name": "podai-platform",
  "version": "1.0.0",
  "description": "Revolutionary AI Agent Platform - 2025 Production Implementation",
  "type": "module",
  "private": true,
  "preferredPackageManager": "bun",
  "packageManager": "bun@1.2.15",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.15"
  },
  "scripts": {
    "build": "bun run build:parallel",
    "build:parallel": "concurrently \"bun run build:rust\" \"bun run build:typescript\" \"bun run build:wasm\"",
    "build:rust": "cd packages/core && cargo build --release",
    "build:typescript": "concurrently \"bun run build:cli\" \"bun run build:sdk-ts\"",
    "build:cli": "cd packages/cli && bun run build",
    "build:sdk-ts": "cd packages/sdk-typescript && bun run build",
    "build:wasm": "cd packages/core && wasm-pack build --target bundler --out-dir ../wasm",
    "build:production": "bun run clean && bun run build && bun run test:critical",
    "build:optimized": "RUSTFLAGS='-C target-cpu=native' bun run build:rust && bun run build:typescript",
    "dev": "concurrently \"bun run dev:cli\" \"bun run dev:sdk-ts\" \"bun run dev:docs\"",
    "dev:cli": "cd packages/cli && bun run dev",
    "dev:sdk-ts": "cd packages/sdk-typescript && bun run dev",
    "dev:docs": "bun run docs:serve",
    "dev:watch": "concurrently \"bun run build:typescript --watch\" \"bun run test:watch\"",
    "test": "bun run test:comprehensive",
    "test:comprehensive": "concurrently \"bun run test:rust\" \"bun run test:typescript\" \"bun run test:integration\" \"bun run test:security\"",
    "test:rust": "cd packages/core && cargo test --release",
    "test:typescript": "concurrently \"bun run test:cli\" \"bun run test:sdk-ts\"",
    "test:cli": "cd packages/cli && bun test",
    "test:sdk-ts": "cd packages/sdk-typescript && bun test",
    "test:integration": "bun test tests/",
    "test:e2e": "bun test tests/e2e/ --timeout 60000",
    "test:security": "bun run audit:all",
    "test:performance": "bun test tests/performance/ --timeout 120000",
    "test:critical": "bun run test:rust && bun run test:security",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "bun run lint:comprehensive",
    "lint:comprehensive": "concurrently \"bun run lint:rust\" \"bun run lint:typescript\" \"bun run lint:docs\" \"bun run lint:config\"",
    "lint:rust": "cd packages/core && cargo clippy -- -D warnings",
    "lint:typescript": "eslint 'packages/**/*.{ts,js}' --fix",
    "lint:docs": "markdownlint '**/*.md' --fix",
    "lint:config": "bun run validate:config",
    "lint:fix": "bun run lint:comprehensive",
    "format": "bun run format:comprehensive",
    "format:comprehensive": "concurrently \"bun run format:rust\" \"bun run format:typescript\" \"bun run format:docs\"",
    "format:rust": "cd packages/core && cargo fmt",
    "format:typescript": "prettier --write 'packages/**/*.{ts,js,json}' --config .prettierrc",
    "format:docs": "prettier --write '**/*.md' --config .prettierrc",
    "audit": "bun run audit:all",
    "audit:all": "concurrently \"bun run audit:rust\" \"bun run audit:bun\" \"bun run audit:npm\" \"bun run audit:licenses\"",
    "audit:rust": "cd packages/core && cargo audit",
    "audit:bun": "bun audit --audit-level moderate",
    "audit:npm": "npm audit --audit-level moderate",
    "audit:licenses": "license-checker --summary",
    "audit:deps": "bun outdated",
    "deps:check": "concurrently \"bun run deps:check:rust\" \"bun run deps:check:bun\"",
    "deps:check:rust": "cd packages/core && cargo outdated",
    "deps:check:bun": "bun outdated",
    "deps:update": "bun run deps:update:safe",
    "deps:update:safe": "bun update --latest && cd packages/core && cargo update",
    "deps:update:major": "bunx npm-check-updates -u && bun install",
    "docs": "bun run docs:generate",
    "docs:generate": "concurrently \"bun run docs:api\" \"bun run docs:rust\" \"bun run docs:architecture\"",
    "docs:api": "cd packages/sdk-typescript && bun run docs",
    "docs:rust": "cd packages/core && cargo doc --no-deps",
    "docs:architecture": "echo 'Architecture docs generation'",
    "docs:serve": "bunx http-server docs/ -p 8080 -c-1",
    "docs:preview": "bun run docs:generate && bun run docs:serve",
    "deploy": "bun run deploy:production",
    "deploy:production": "bun run build:production && bun run deploy:contracts",
    "deploy:contracts": "cd packages/core && anchor deploy",
    "deploy:devnet": "cd packages/core && anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "cd packages/core && anchor deploy --provider.cluster mainnet-beta",
    "clean": "bun run clean:comprehensive",
    "clean:comprehensive": "concurrently \"bun run clean:rust\" \"bun run clean:typescript\" \"bun run clean:cache\"",
    "clean:rust": "cd packages/core && cargo clean",
    "clean:typescript": "rimraf 'packages/*/{dist,build,.next,coverage}' node_modules/.cache",
    "clean:cache": "rimraf node_modules/.cache ~/.bun/install/cache",
    "fresh": "bun run clean && bun install && bun run build",
    "reset": "bun run clean && rm -rf node_modules packages/*/node_modules bun.lock && bun install",
    "validate": "bun run validate:comprehensive",
    "validate:comprehensive": "concurrently \"bun run validate:config\" \"bun run validate:types\" \"bun run validate:security\"",
    "validate:config": "echo 'Config validation'",
    "validate:types": "tsc --project tsconfig.validation.json",
    "validate:security": "bun run audit:all",
    "validate:build": "bun run build && bun run test:critical",
    "monitor": "bun run monitor:comprehensive",
    "monitor:comprehensive": "concurrently \"bun run monitor:performance\" \"bun run monitor:security\" \"bun run monitor:deps\"",
    "monitor:performance": "echo 'Performance monitoring'",
    "monitor:security": "echo 'Security monitoring'",
    "monitor:deps": "bunx depcheck",
    "autofix": "node .scripts/run-all-fixes.js",
    "autofix:quick": "node .scripts/run-all-fixes.js --quick",
    "autofix:typescript": "node .scripts/auto-fix/typescript-fixer.js",
    "autofix:config": "node .scripts/auto-fix/config-fixer.js",
    "autofix:deps": "node .scripts/auto-fix/dependency-resolver.js",
    "generate:code": "node .scripts/dev-accelerators/ai-code-generator.js",
    "setup:windows": "powershell -ExecutionPolicy Bypass .scripts/environment/windows-setup.ps1",
    "publish": "bun run publish:all",
    "publish:all": "concurrently \"bun run publish:cli\" \"bun run publish:sdk-ts\"",
    "publish:cli": "cd packages/cli && npm publish",
    "publish:sdk-ts": "cd packages/sdk-typescript && npm publish",
    "pre-commit": "bun run lint && bun run test:critical"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@jest/globals": "^30.0.3",
    "@solana/web3.js": ">=1.90.0",
    "@types/bun": "^1.2.17",
    "@types/node": "24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/ui": "3.2.4",
    "audit-ci": "^7.1.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.0",
    "depcheck": "^1.4.7",
    "docsify-cli": "^4.4.4",
    "esbuild": "0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "3.0.4",
    "globals": "^16.2.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.3",
    "jsdoc": "^4.0.4",
    "license-checker": "^25.0.1",
    "markdownlint-cli": "0.45.0",
    "nodemon": "^3.1.10",
    "npm-check-updates": "^18.0.1",
    "playwright": "^1.48.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.0",
    "size-limit": "^11.2.0",
    "solana-bankrun": "^0.4.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "vitest": "3.2.4",
    "wasm-pack": "^0.13.1"
  },
  "dependencies": {
    "@codama/nodes-from-anchor": "^1.2.1",
    "@codama/renderers-js": "^1.2.14",
    "@solana-program/compute-budget": "^0.8.0",
    "@solana-program/system": "^0.7.0",
    "@types/js-yaml": "^4.0.9",
    "chalk": "^5.3.0",
    "codama": "^1.3.0",
    "commander": "^14.0.0",
    "js-yaml": "^4.1.0",
    "ora": "^8.1.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.90.0",
    "typescript": ">=5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"],
    "*.rs": ["cargo fmt --"],
    "Cargo.toml": ["cargo check"]
  },
  "size-limit": [
    {
      "path": "packages/sdk-typescript/dist/index.js",
      "limit": "50 KB"
    },
    {
      "path": "packages/cli/dist/index.js",
      "limit": "100 KB"
    }
  ],
  "keywords": [
    "ai-agents",
    "blockchain",
    "solana",
    "web3",
    "protocol",
    "podai",
    "zk-compression",
    "federated-learning",
    "p2p",
    "webassembly",
    "typescript",
    "rust"
  ],
  "author": "podAI Team <dev@podai.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/podai/platform.git"
  },
  "bugs": {
    "url": "https://github.com/podai/platform/issues"
  },
  "homepage": "https://podai.com"
}